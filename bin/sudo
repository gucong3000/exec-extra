#!/usr/bin/env node
require('../');
var runas = require('runas');
var result = runas(process.argv[2], process.argv.slice(3), {
	admin: true,
	catchOutput: true,
});

if(result.stdout) {
	process.stdout.write(result.stdout);
}

if(result.stderr) {
	process.stderr.write(result.stderr);
}

process.exit(result.exitCode);
